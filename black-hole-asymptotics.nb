  ";"}], "\n", 
 RowBox[{
  RowBox[{"gradU", "=", 
   RowBox[{"Grad", "[", 
    RowBox[{"U", ",", 
     RowBox[{"{", 
      RowBox[{"r", ",", "\[Theta]", ",", "z"}], "}"}], ",", 
     "\"\<Cylindrical\>\""}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"strainU", "=", 
   RowBox[{"gradU", "+", 
    RowBox[{"gradU", "\[Transpose]"}]}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"divU", "=", 
   RowBox[{"Div", "[", 
